                "summary": "Returns a list of users by the given name",
                "description": "",
                "operationId": "getUsersByName",
                "parameters": [
                    {
                        "name": "firstName",
                        "in": "path",
                        "description": "The first name of the user that should be returned",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    }
                                }
                            }
                        }
                    }
                }
            }
