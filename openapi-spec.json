                "summary": "Adds a new user to the server with the given information in the body",
                "description": "",
                "parameters": [],
                "operationId": "createNewUser",
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
